#include <stdio.h>

int main() {
    int n; // Number of elements in the set
    int i; // Loop counter
    double sum = 0.0; // Variable to store the sum
    double element;
    // Input the number of elements
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    // Input the elements and calculate the sum
    for (i = 1; i <= n; ++i) {
        
        printf("Enter element %d: ", i);
        scanf("%lf", &element);

        sum += element; // Accumulate the sum
    }

    // Display the result
    printf("Sum of the set of numbers = %.2lf\n", sum);

    return 0;
}
