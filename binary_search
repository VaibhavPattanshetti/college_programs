#include <stdio.h>
int main()
{
int arr[100],no,i=0,n,m,j,k;
printf("Enter the size of an array");
scanf("%d",&no);

for( int i=0;i<=no;i++)
{
	printf("Enter array elements of array at (ascending order) position of %d\n",i);
	scanf("%d",&arr[i]);
}
printf("To print array elements type 1\n");
scanf("%d",&n);
if(n==1)
{
for( int i=0;i<=no;i++)
{
	printf("%d\n",arr[i]);

}
}
int low=0,high=no,mid;
printf("Enter elements for search in array\n");
scanf("%d",&m);

for( int i=0;i<=no;i++)
{
	
mid=(low+high)/2;
if(arr[mid]==m)
{printf("Element is at %d",mid);
break;
}
else if(arr[mid]>m)
{
high=mid;
}
else if(arr[mid]<m)
{
low=mid;
}
}
return 0;
}







