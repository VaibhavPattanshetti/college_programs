#include<stdio.h>
#include<limits.h>
#define size 5
int stack[size];
int top=-1;
void push(int element){
if(top==size-1){
printf("satck is full");
return;
}
stack[++top]=element;
return;
}
int pop(){
int element;
if(top==-1){
printf("stack is empty");
return 0;
}
element=stack[top];
top--;
return element;
}
int main(){
push(50);
push(45);
push(56);
push(6);

printf("%d\n",pop());
printf("%d\n",pop());
printf("%d\n",pop());
printf("%d\n",pop());
return 0;
}
