 #include <stdio.h>
 int main()
 {
    int num,result=0;

    printf("enter the integer greater than 1:");
    scanf("%d",&num);
    for(int i=2;i<=num;i++){
        if(num%i ==0){
            result=i;
            break;
        }
    }

    printf("The smallest divisor of %d is: %d",num,result);

    return 0;
 }
